{
  "name": "@jdizm/vue-storybook",
  "version": "2.0.0",
  "private": false,
  "main": "./dist/index.umd.min.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/entry.js --name index",
    "build:inline": "vue-cli-service build --target lib src/entry.js --inline-vue",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006 -s ./stories/assets",
    "test:unit": "vue-cli-service test:unit",
    "test:update": "vue-cli-service test:unit -u",
    "test:coverage": "vue-cli-service test:unit --collectCoverage",
    "test:verbose": "vue-cli-service test:unit --verbose"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
